<launch>
  <arg
    name="model" />
  <arg
    name="gui"
    default="False" />
  <param
    name="robot_description"
    textfile="$(find urdf_package)/urdf/Part1.urdf" />
  <param
    name="use_gui"
    value="$(arg gui)" />
  <node
    name="rviz"
    pkg="rviz"
    type="rviz"
    args="-d $(find urdf_package)/urdf.rviz" />
</launch>
